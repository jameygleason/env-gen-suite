{
  "name": "theme-engine",
  "description": "Theme Engine",
  "version": "0.0.1",
  "scripts": {
    "start": "node __sapper__/build",
    "dev": "node -r esm node_modules/sapper/dist/cli.js dev",
    "build": "cross-env NODE_ENV=production sapper build",
    "export": "cross-env NODE_ENV=production sapper export",
    "test": "run-p --race cy:open cy:run",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "push": "sh ./scripts/push.sh"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "dotenv": "8.2.0",
    "fastify": "3.14.2",
    "fastify-compress": "3.4.2",
    "fastify-cors": "5.2.0",
    "kleur": "4.1.4",
    "middie": "5.2.0",
    "node-fetch": "2.6.1",
    "sirv": "1.0.11"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@babel/eslint-parser": "7.14.2",
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-replace": "2.4.2",
    "@rollup/plugin-url": "6.0.0",
    "@signalchain/rollup-plugin-env-gen": "file:../../package",
    "@signalchain/svelte-forms": "0.1.4",
    "@signalchain/utils": "0.1.6",
    "@testing-library/cypress": "7.0.6",
    "cross-env": "7.0.3",
    "cssnano": "5.0.2",
    "cypress": "7.3.0",
    "cypress-plugin-tab": "1.0.5",
    "eslint": "7.26.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-json": "3.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-svelte3": "3.2.0",
    "esm": "3.2.25",
