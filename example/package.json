{
  "name": "theme-engine",
  "description": "Theme Engine",
  "version": "0.0.1",
  "scripts": {
    "start": "node __sapper__/build",
    "dev": "cross-env PACKAGE_DEV=true sapper dev",
    "build": "cross-env NODE_ENV=production sapper build",
    "export": "cross-env NODE_ENV=production sapper export",
    "test": "run-p --race cy:open cy:run",
    "generate": "node -r esm ./sapper-generate.js",
    "generate:dev": "cross-env NODE_ENV=development PACKAGE_DEV=true node -r esm packages/index.js",
    "generate:prod": "cross-env NODE_ENV=production node -r esm packages/index.js",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "push": "sh ./scripts/push.sh"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=14.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "fastify": "3.14.2",
    "fastify-compress": "3.4.2",
    "fastify-cors": "5.2.0",
    "kleur": "4.1.4",
    "middie": "5.2.0",
    "node-fetch": "2.6.1",
    "sirv": "1.0.11"
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@babel/eslint-parser": "7.13.14",
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-replace": "2.4.2",
    "@rollup/plugin-url": "6.0.0",
    "@signalchain/svelte-forms": "0.1.4",
    "@signalchain/utils": "0.1.6",
    "@testing-library/cypress": "7.0.6",
    "cross-env": "7.0.3",
    "cssnano": "5.0.1",
    "cypress": "7.1.0",
    "cypress-plugin-tab": "1.0.5",
    "eslint": "7.25.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-svelte3": "3.2.0",
    "esm": "3.2.25",
    "fast-glob": "3.2.5",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "lodash.merge": "4.6.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.2.12",
    "postcss-easings": "3.0.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "prettier-plugin-svelte": "2.2.0",
    "rollup": "2.45.2",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "sapper": "0.29.1",
    "sass": "1.32.11",
    "svelte": "3.37.0",
    "svelte-preprocess": "4.7.2",
    "svelte-select": "3.17.0"
  }
}
